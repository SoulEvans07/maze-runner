{
  "name": "maze-runner",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:clean": "turbo build:clean",
    "build:fe": "turbo build --filter=frontend",
    "build:be": "turbo build --filter=server",
    "dev": "turbo dev",
    "dev:fe": "turbo dev --filter=frontend",
    "dev:be": "turbo dev --filter=server",
    "start": "turbo start",
    "start:fe": "turbo start --filter=frontend",
    "start:be": "turbo start --filter=server",
    "lint": "turbo lint",
    "graph:build": "turbo build --graph",
    "prettier": "turbo prettier"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.15.7"
}
